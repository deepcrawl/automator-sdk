{
  "name": "automator-tools-azure-pipelines-extension",
  "version": "1.0.2",
  "description": "Azure task for running the build of Protect/Lumar test suite.",
  "main": "index.js",
  "scripts": {
    "preinstall": "yarn audit",
    "build": "ttsc",
    "start": "node ./dist/index.js",
    "lint": "yarn prettier --write . && yarn eslint --ext .js,.ts src",
    "test": "jest --maxWorkers=1",
    "test:integration": "jest --config jest.config.integration.js --maxWorkers=1",
    "test:integration:watch": "yarn test:integration --watch",
    "test:unit": "jest --config jest.config.test.js --maxWorkers=4",
    "test:unit:watch": "yarn test:unit --watch"
  },
  "author": "Andrei Bitca, Daniel Mon Chit & Lukas Kalcok",
  "license": "ISC",
  "resolutions": {
    "cross-fetch": "3.1.5",
    "minimatch": "7.4.2",
    "ws": "8.12.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node-fetch": "^2.6.2",
    "@types/q": "^1.5.4",
    "@types/sinon": "^9.0.8",
    "eslint": "^8.35.0",
    "eslint-config-deepcrawl": "^8.1.0",
    "eslint-plugin-graphql": "^4.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-fetch-mock": "^3.0.3",
    "nock": "^13.3.0",
    "prettier": "^2.1.2",
    "sinon": "^9.2.0",
    "ts-jest": "^26.4.1",
    "ttypescript": "^1.5.15",
    "typescript": "^4.0.3",
    "typescript-transform-paths": "3.4.6",
    "y18n": "4.0.1"
  },
  "dependencies": {
    "@apollo/client": "^3.2.2",
    "axios": "^0.21.1",
    "azure-pipelines-task-lib": "^4.3.0",
    "graphql": "^16.6.0",
    "node-fetch": "^2.6.9",
    "winston": "3.8.2"
  }
}
