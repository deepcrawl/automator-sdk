{
  "manifestVersion": 1,
  "id": "automator-tools",
  "name": "Automator Tools",
  "version": "1.0.3",
  "publisher": "deepcrawl",
  "targets": [{
    "id": "Microsoft.VisualStudio.Services"
  }],    
  "description": "A collection of tasks used for interacting with Automator.",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "automator",
    "deepcrawl",
    "build"
  ],
  "content": {
    "details": {
        "path": "README.md"
    }
  },
  "icons": {
    "default": "images/extension-icon.png"        
  },
  "files": [{
    "path": "run-automator-build-task"
  }],
  "contributions": [{
    "id": "run-automator-build-task",
    "type": "ms.vss-distributed-task.task",
    "targets": [
      "ms.vss-distributed-task.tasks"
    ],
    "properties": {
      "name": "run-automator-build-task"
    }
  }]
}
